<%= simple_form_for(@bar) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :price %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :address %>
    <%= f.input :happy %>
    <%= f.input :start_happy %>
    <%= f.input :end_happy %>
    <%= f.input :enable %>
    <%= f.input :partner %>
    <%= f.input :phone %>
    <%= f.input :facebook %>
    <%= f.input :twitter %>
    <%= f.input :site %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
